<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@WelcomeWindowCSS.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.WelcomeWindowController">
   <children>
      <Rectangle fx:id="rectangle" arcHeight="5.0" arcWidth="5.0" fill="#a6000f" height="100.0" stroke="BLACK" strokeType="INSIDE" width="600.0" />
      <Button fx:id="btn_teacher" mnemonicParsing="false" onAction="#clickTeacher" text="Teacher" AnchorPane.bottomAnchor="50.0" AnchorPane.rightAnchor="100.0" />
      <Label fx:id="lbl1" layoutX="272.0" layoutY="126.0" text="Hello!" AnchorPane.topAnchor="126.0" />
      <Label fx:id="lbl2" layoutX="153.0" layoutY="200.0" text="Are you a student or a teacher?" AnchorPane.topAnchor="200.0" />
      <Button fx:id="btn_student" mnemonicParsing="false" onAction="#clickStudent" text="Student" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="100.0" />
      <Button fx:id="btn_close" layoutX="528.0" layoutY="350.0" mnemonicParsing="false" text="Close" AnchorPane.bottomAnchor="7.0" AnchorPane.rightAnchor="7.0" />
   </children>
</AnchorPane>
